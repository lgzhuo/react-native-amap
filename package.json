{
  "name": "react-native-amap",
  "description": "React Native AMap component for iOS + Android",
  "main": "index.js",
  "version": "0.5.0",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:android": "adb shell am start -n rct.amap.example/.MainActivity",
    "run:android": "./gradlew installDebug && npm run start:android",
    "run:ios": "react-native run-ios --project-path ./example/ios"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "babel-jest": "22.2.2",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.2.2",
    "react": "16.2.0",
    "react-native": "0.53.3",
    "react-navigation": "^1.1.2",
    "react-test-renderer": "16.2.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "urijs": "^1.18.10"
  }
}
